<!DOCTYPE html>
<html>
    <head>
        <meta charset="utf-8">
        <title>Weather web</title>
        <link rel="stylesheet" href="index1.css">
        
    </head>
    <body>
       
        <div class="full_screen">
           
            <div class="current">
                <div class="time-place">
                    <div class="time">
                        <div class="hours" id="hours_id">20.30 </div>
                        <div class="date" id="date_id">5/6/22</div>
    
                    </div>
    
                   
                
                    <div class="place">
                        <div class="city"> <%=loc%> </div>
                        <div class="country"><%= countryel %>  </div>
                    </div>
    
                </div>
               
                <div class="todays">
                    <img src="icon.png" alt="weather icon" class="w-icon">
                    <div class="other-today">
                        <p class="day" id="day_id">Tuesday</p>
                        <p class="temp"><%= tempel %> &#176C</p>
                        <p class="weather-description"><%= desc %></p>
                        
                    </div>
    
                </div>

            </div>
            <div class="container">
                <div class="other-info">
                    
                    <p class="item"><%= feel %> &#176C</p>
                    <p class="item"><%= cloudel %> </p>
                    <p class="item"><%= humel %> </p>
                    <p class="item"><%= mini %> &#176C</p>
                    <p class="item"><%= maxi %> &#176C</p>
                    <p class="item"><%= pressel %></p>
                    <p class="item"><%= wind %></p>
                   
                    
                </div>
            </div>
            
            

        </div>
       
    
    <script src="script.js"></script>
</body>
</html>

        <script src="index.js"></script>
    </body>
</html>